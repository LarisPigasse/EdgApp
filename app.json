{
  "expo": {
    "name": "EdgApp",
    "slug": "edgapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#00aefa"
      },
      "package": "com.expressdeliverygroup.edgapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Consenti all'app di accedere alla fotocamera per scattare foto delle spedizioni."
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Consenti all'app di accedere alla fotocamera per scansionare i codici QR."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Consenti all'app di accedere alla tua posizione per registrare la posizione delle spedizioni.",
          "locationForegroundPermission": "Consenti all'app di accedere alla tua posizione quando l'app Ã¨ in uso."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "222d1a3c-20e7-4088-9278-1247b7d1876a"
      }
    }
  }
}
